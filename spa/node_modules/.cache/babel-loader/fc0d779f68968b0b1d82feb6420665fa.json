,KAAA,GAAA,IAAA,MAAA,CAAA,YAAA,CAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,WAAA,IAAA;AACA,GAFA,MAEA,IAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,GAAA,EAAA,EAAA;AACA,aAAA,KAAA;AACA;;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,aAAA,KAAA;AACA;;AACA,WAAA,IAAA;AACA,GARA,MAQA;AACA,WAAA,KAAA;AACA;AACA,C,CACA;;;AACA,SAAA,OAAA,CAAA,GAAA,EAAA,GAAA,EAAA;AACA,MAAA,MAAA,GAAA,EAAA;;AACA,OAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,GAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,QAAA,GAAA,CAAA,UAAA,CAAA,CAAA,KAAA,EAAA,IAAA,GAAA,CAAA,UAAA,CAAA,CAAA,KAAA,EAAA,EAAA;AACA,MAAA,MAAA,IAAA,MAAA,CAAA,YAAA,CAAA,CAAA,GAAA,CAAA,UAAA,CAAA,CAAA,IAAA,EAAA,GAAA,GAAA,GAAA,EAAA,IAAA,EAAA,GAAA,EAAA,CAAA;AACA,KAFA,MAGA,IAAA,GAAA,CAAA,UAAA,CAAA,CAAA,KAAA,EAAA,IAAA,GAAA,CAAA,UAAA,CAAA,CAAA,KAAA,GAAA,EAAA;AACA,MAAA,MAAA,IAAA,MAAA,CAAA,YAAA,CAAA,CAAA,GAAA,CAAA,UAAA,CAAA,CAAA,IAAA,EAAA,GAAA,GAAA,GAAA,EAAA,IAAA,EAAA,GAAA,EAAA,CAAA;AACA,KAFA,CAGA;AAHA,SAIA,MAAA,IAAA,MAAA,CAAA,YAAA,CAAA,GAAA,CAAA,UAAA,CAAA,CAAA,CAAA,CAAA;AACA,GAXA,CAYA;;;AACA,SAAA,MAAA;AACA","sourcesContent":["<template>\r\n<el-main class=\"backbig\">\r\n<div class=\"innerback\">\r\n<p class=\"denglu\">登录</p>\r\n<el-input v-model=\"username\"   clearable placeholder=\"请输入账号\"></el-input>\r\n<el-input v-model=\"password\" show-password placeholder=\"请输入密码\"></el-input>\r\n<el-button style=\"width: 150px;\" type=\"primary\" plain @click=\"login\">登录</el-button>\r\n<p class=\"msg\">{{message}}</p>\r\n</div>\r\n</el-main>\r\n      \r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n    name: \"Login\",\r\n    data() {\r\n        return {      \r\n           username: '',\r\n\t\t\tpassword: '',\r\n            message:''\r\n        };\r\n    },\r\n\r\n   \r\n\r\n  methods: {\r\n    login() {\r\n      if(isNotGood(this.username)){\r\n      this.axios({\r\n        method:\"POST\",\r\n        //发生请求地址\r\n        url:\"/api/login\",\r\n        //发送数据\r\n        data:{\r\n            \"username\":this.username,\r\n            \"password\":encrypt(this.password)\r\n        }\r\n        //响应结果\r\n       }).then(response=>{\r\n           if (response.data.status == \"good\") {\r\n            this.$router.push('/main');\r\n          } else {\r\n            this.message = response.data.errMsg\r\n          }\r\n       })\r\n      }else {\r\n     this.message = \"您输入的用户名有误！\"\r\n      }\r\n    },\r\n  },\r\n};\r\n/**\r\n   * 判断输入是不是非法的\r\n   */\r\n   function isNotGood(data) {\r\n    var patt1=new RegExp(/^1\\d{10}$/);\r\n    var patt2=new RegExp(/^[a-zA-Z._-]*[a-zA-Z]+[0-9]*@bank.c(om|n)$/);\r\n    var patt3=new RegExp(/[._-][._-]/);\r\n    if(patt1.test(data)){\r\n        return true;\r\n    }else if(patt2.test(data)){\r\n        if(data.length>30){\r\n          return false;\r\n        }\r\n        if(patt3.test(data)){\r\n          return false;\r\n        }\r\n        return true;\r\n      }else{\r\n        return false;\r\n      }\r\n  }\r\n    // 加密\r\n    function encrypt(str, num) {\r\n      var newStr = \"\";\r\n      for (let i = 0; i < str.length; i++) {\r\n        if (str.charCodeAt(i) >= 65 && str.charCodeAt(i) <= 90) {\r\n          newStr += String.fromCharCode((str.charCodeAt(i) - 65 + num + 26) % 26 + 65)\r\n        }\r\n        else if (str.charCodeAt(i) >= 97 && str.charCodeAt(i) <= 122) {\r\n          newStr += String.fromCharCode((str.charCodeAt(i) - 97 + num + 26) % 26 + 97)\r\n        }\r\n        //特殊符号不做处理\r\n        else newStr += String.fromCharCode(str.charCodeAt(i));\r\n      }\r\n      // console.log(newStr);\r\n      return newStr;\r\n    }\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.backbig{    \r\nheight: 100vh;\r\nwidth: 100vw;\r\nbackground-image: url(../assets/login.png);\r\nbackground-size: 100% 100%;\r\n}\r\n.innerback{\r\nmax-width: fit-content;\r\nheight: 300px;\r\nwidth: 300px;\r\nmargin: 15vh auto;\r\ntext-align: center;\r\n}\r\n.denglu {\r\n    text-align: center;\r\n\tcolor: white;\r\n\tfont-size: 43px;\r\n}\r\n.msg{\r\n     margin-top: 20px;\r\n    text-align: center;\r\n\tcolor: orange;\r\n\tfont-size: 25px;\r\n}\r\n.el-input{\r\n    margin-top: 20px;\r\n}\r\n.el-button{\r\n    margin-top: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}